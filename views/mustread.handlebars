<div class="col s12 m12 l3" style="border: 2px solid">
            <aside id="mustread" class="aside">
                <h3>Must Read</h3>
                <div id="must-read">
                    {{#each books as |book|}}
                    <h5>{{book.title}}</h5>
                    <p>by {{book.author}}</p>
                    <p>published {{book.year_published}}</p>
                    {{/each}}
                </div>
            </aside>
</div>